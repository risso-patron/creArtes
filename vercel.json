{
  "version": 2,
  "name": "studio-creartes-categorizer",
  "builds": [
    {
      "src": "image-categorizer.html",
      "use": "@vercel/static"
    },
    {
      "src": "crearte/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/image-categorizer.html"
    },
    {
      "src": "/crearte/(.*)",
      "dest": "/crearte/$1"
    }
  ],
  "headers": [
    {
      "source": "/crearte/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
